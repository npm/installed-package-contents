  },
  "dependencies": {
    "npm-bundled": "^1.1.1",
    "npm-normalize-package-bin": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/installed-package-contents.git"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || >=16.0.0"
  },
  "templateOSS": {
